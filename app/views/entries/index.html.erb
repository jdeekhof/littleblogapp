
<%= link_to("New Entry", new_entry_path) %>

<ul>
	<% for entry in @entries %>
	    	<h2><%= link_to(entry.title, entry_path(entry)) %></h2>
	    	<p><%= entry.date %></p>
	    	<p><%= entry.contents %></p>
	    	<p><%= link_to("Edit", edit_entry_path(entry)) %></p>
	<% end %>
</ul>


